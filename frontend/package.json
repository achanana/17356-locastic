{
  "name": "loctastic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@geist-ui/react": "^2.1.5",
    "@material-ui/core": "^4.11.4",
    "@material-ui/styles": "^4.11.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "cross-spawn": "^7.0.3",
